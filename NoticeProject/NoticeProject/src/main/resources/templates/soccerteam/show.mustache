{{>layouts/header}}

<div class="card mb-4">
			<div class="card-body">
				{{#soccerteams}}
					<div class="mb-3 mt-3">
						<label for="views" class="form-label">조회수</label> 
						<input type="text" class="form-control" value={{views}} disabled>
					</div>
					
					<div class="mb-3">
						<label for="title" class="form-label">제목</label> 
						<input type="text" class="form-control" value={{title}} disabled>
					</div>
					
					<div class="mb-3">
						<label for="content" class="form-label">내용</label> 
						<input type="text" class="form-control" value={{content}} disabled>
					</div>
					
					<div class="mb-3">
						<label for="regdate" class="form-label">작성일</label> 
						<input type="text" class="form-control" value={{created}} disabled>
					</div>
					
					<div class="mb-3">
						<label for="writer" class="form-label">작성자</label> 
						<input type="text" class="form-control" value={{writer}} disabled>
					</div>
					
					<a href="/soccerteams" class="btn btn-outline-secondary">list</a>
					<a href="/soccerteam/{{id}}/edit" class="btn btn-outline-primary">수정하기</a>
					<a href="/soccerteam/{{id}}/delete" class="btn btn-outline-primary">삭제하기</a>
				{{/soccerteams}}
			</div>
</div>

{{>soccerteam_comment/comment}}
{{>layouts/footer}}